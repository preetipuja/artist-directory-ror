<%= form_for @feedback, url: artist_feedbacks_path(@artist) do |f| %>
<div class="form-group row">
  <div class="col-sm-2"><%= f.label :review %></div>
  <div class="col-sm-10"><%= f.text_area :review, class: 'form-control' %></div>
</div>

<div class="form-group row">
  <div class="col-sm-2"><%= f.label :rating %></div>
  <div class="col-sm-10"><%= f.select :rating , [1,2,3,4,5] , {}, :id => "example", class: 'form-control' %></div>
</div>
<div class="form-group row">
  <%= f.submit :class=> "btn btn-primary" %>
</div>
<% end %>
